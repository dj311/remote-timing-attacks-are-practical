 #include <openssl/md5.h>
 #include "cryptlib.h"
 
+
+// djwj:start
+unsigned long long
+get_cycles() {
+        long long out;
+        asm volatile(
+                "CPUID;"
+                "RDTSCP;"
+                "SHLQ $32,%%rdx;"
+                "ORQ %%rdx,%%rax;"
+                "MOVQ %%rax,%0;"
+                :"=r"(out)
+                : /*no input*/
+                :"rdx","rax", "rcx"
+        );
+        return out;
+}
+// djwj:end
+
